/*-------------------------*/
/*头部个人信息*/
.user-section{
    display:flex;
    padding:30rpx 40rpx;
}
.user-section .head-pic{
    width:120rpx;
    height:120rpx;
    border-radius: 50%;
    overflow: hidden;
    margin-right:34rpx;
}
.user-section .head-pic image{
width:100%;
    height:100%
}
.user-section .user-info .text-desc{
    font-size:56rpx;
    line-height:75rpx;
    color:#333;
}
.user-section .user-info .text-tip{
    font-size:26rpx;
    color:#666
}


/*账户信息*/
.amount-section{
  padding:30rpx;
}
.amount-box{
  padding:30rpx;
  display: flex;
  justify-content: space-between;
  background-size: 100%;
  width:100%;
  height:160rpx;
  background-image:url("data:image/png;base64,");
}
.amount-box .amount-info{
  display: flex;
}
.amount-box .left-pic{
  width:40rpx;
  height:40rpx;
  margin-right:32rpx;
}
.amount-box .text-con .text-num{
  font-size:50rpx;
  line-height:50rpx;
  margin-bottom:12rpx;
  color:#fff;
}
.amount-box .text-con .text-tip{
  font-size:30rpx;
  color:#fff;
}
.amount-box .btn-charge{
  margin:auto 0;
  height:50rpx;
  border-radius: 25rpx;
  color:#4C8EFF;
  font-size:24rpx;
  line-height:50rpx;
  margin-top:22rpx;
}

/*快捷入口*/
.quick-section{
  width:100%;
  padding:0 30rpx;
  display: flex;
  justify-content: space-between;
}
.quick-section .bill-box,.quick-section .carbind-box{
  display: flex;
  justify-content: space-between;
  border-radius: 10rpx;
  width:42%;
  box-shadow: 0rpx 0rpx 10rpx 0rpx rgba(161, 199, 248, 0.6);
  padding:24rpx
}
.quick-section .text-desc{
  font-size:32rpx;
  color:#333;margin-right:4rpx}

.quick-section .text-tip{
  font-size:24rpx;
  color:#999;
}


.quick-section .right-icon image{
  width:60rpx;
  height:62rpx;
}

/*列表*/
.list-section{
  padding:50rpx 0 50rpx 42rpx;
}
.list-cell{
  display: flex;
  justify-content: space-between;
  align-items: center;
  border-bottom:1px solid #eeeeee;
  height:90rpx;
}
.list-cell .text-desc{
  font-size:32rpx;color:#333;
}
.list-cell .text-tip{
  font-size:24rpx;color:#999;
}
.list-cell .right-con{margin-right:40rpx;}
.list-cell .list-arrow{width:12rpx;height:22rpx;margin-left:16rpx;}
.btn-exit{
  width:150rpx;height:60rpx;border-radius:30rpx;border:1px solid #cccccc;color:#333;font-size:28rpx;text-align: center;background:#fff;line-height:60rpx;
}
.exit-bar{display:flex;justify-content:space-around;padding-bottom:40rpx;}



/**发票提示对话框**/
.dialog-float-layer {
    width: 539rpx;
    height: 612rpx;
    z-index: 102;
    position: fixed;
    top: 50%;
    left: 50%;
    transform: translate(-50%,-50%);
    text-align:center;
}

.dialog-float-layer-close {
    width: 60rpx;
    height: 60rpx;
    position: absolute;
    bottom: -88px;
    left: 50%;
    transform: translateX(-50%);
}

.invoicedialog-content{
    display: flex;
    flex-direction: column;
    justify-content: center;
    align-items: center;
}

.invoicedialog-content-top{
    display: flex;
    justify-content: center;
    align-items: center;
    background: linear-gradient(to right,#4C8EFF,#7AC0FF);
    border-top-left-radius: 36rpx;
    border-top-right-radius: 36rpx;
    width: 100%;
}

.invoicedialog-content-bottom{
    display: flex;
    flex-direction: column;
    justify-content: center;
    background: #fff;
    border-bottom-left-radius: 36rpx;
    border-bottom-right-radius: 36rpx;
    width: 100%;
}

.invoicedialog-btnpay{
    background: linear-gradient(to right,#4C8EFF,#7AC0FF);
    color:#fff;
    font-size:14px;
    width:308rpx;
    border-radius:22px;
    line-height:90rpx;
    z-index:10;
    height:90rpx;
    margin-top:41rpx;
    margin-bottom:24rpx;
}

.invoicedialog-content-top-txt{
    color:white;
    font-size:20px;
    margin:24rpx 0;
}

.invoicedialog-content-view{
    display: flex;
    flex-direction: row;
    margin-top: 36rpx;
}

.invoicedialog-dot{
    width:16rpx;
    height:16rpx;
    background: linear-gradient(to right,#4C8EFF,#7AC0FF);
    border-radius:50%;
    transition:all .6s;
    margin-left:36rpx;
    margin-top:10rpx;
}

.invoicedialog-content-txt{
    font-size: 24rpx;
    color:#000000;
    margin-left:12rpx;
    font-style:bold;
    margin-right:24rpx;
    text-align:left;
    width:90%;
}


.dialog-mask{
    width: 100%;
    height: 100%;
    background-color: rgba(0,0,0,0.24);
    z-index: 101;
    position: fixed;
    left: 0;
    top: 0;
}


